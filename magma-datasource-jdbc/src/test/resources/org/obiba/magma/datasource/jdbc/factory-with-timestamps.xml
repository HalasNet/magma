<org.obiba.magma.datasource.jdbc.JdbcDatasourceFactory>
  <name>my-datasource</name>
  <jdbcProperties>
    <property name="driverClassName" value="org.hsqldb.jdbcDriver"/>
    <property name="url" value="jdbc:hsqldb:mem:datasource_jdbc"/>
    <property name="username" value="sa"/>
    <property name="password" value=""/>
  </jdbcProperties>
  <settings>
    <defaultEntityType>Participant</defaultEntityType>
    <defaultCreatedTimestampColumnName>created</defaultCreatedTimestampColumnName>
    <defaultUpdatedTimestampColumnName>updated</defaultUpdatedTimestampColumnName>
    <tableSettings>
      <org.obiba.magma.datasource.jdbc.JdbcValueTableSettings>
        <sqlTableName>override_names</sqlTableName>
        <entityType>test</entityType>
        <entityIdentifierColumns>
          <string>entity_id</string>
        </entityIdentifierColumns>
        <createdTimestampColumnName>inserted</createdTimestampColumnName>
        <updatedTimestampColumnName>lastUpdate</updatedTimestampColumnName>
      </org.obiba.magma.datasource.jdbc.JdbcValueTableSettings>
    </tableSettings>
  </settings>
</org.obiba.magma.datasource.jdbc.JdbcDatasourceFactory>
